$sequence "headturn_yaw" { //left, right
	"a_headturn_lt"
	"a_headturn_ct"
	"a_headturn_rt"
	autoplay
	blend "head_yaw" 60 -60
	blendwidth 3
	delta
	fadein 0.2
	fadeout 0.2
	hidden
	fps 30	
}

$sequence "headturn_pitch" { //up, down
	"a_headturn_up"
	"a_headturn_ct"
	"a_headturn_dn"
	autoplay
	blend "head_pitch" -35 35 
	blendwidth 3
	delta
	fadein 0.2
	fadeout 0.2
	hidden
	fps 30
	
}

$sequence "headturn_roll" { //tilt; roll; right, tilt; roll; left
	"a_headturn_roll_lt"
	"a_headturn_ct"
	"a_headturn_roll_rt"
	autoplay
	blend "head_roll" 15 -15
	blendwidth 3
	delta
	fadein 0.2
	fadeout 0.2
	hidden
	fps 30
	
}