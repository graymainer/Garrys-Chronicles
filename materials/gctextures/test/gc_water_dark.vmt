	"proxies"
	{
		"TextureScroll"
		{
			"texturescrollvar" "$Envmapmasktransform"
			"texturescrollrate" 0.025
			"texturescrollangle" 300
		}
		"TextureScroll"
		{
			"texturescrollvar" "$bumpTransform"
			"texturescrollrate" 0.025
			"texturescrollangle" 300
		}
		"TextureScroll"
		{
			"Texturescrollvar" "$Basetexturetransform"
			"Texturescrollrate" "0.025"
			"texturescrollangle" "300"
		}

	}


}