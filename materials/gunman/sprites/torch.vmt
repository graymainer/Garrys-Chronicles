"Sprite"
{
	"$basetexture" "gunman/sprites/torch"
	"$spriteorientation" "vp_parallel"
	"$spriteorigin" "[ 0.50 0.90 ]"
	"$nocull" "1"
	"$vertexcolor" "1"
	"$vertexalpha" "1"
	"%keywords" "gunman"
	"$translucent" 1
	
//animation
	Proxies
	{ 
		"AnimatedTexture" 
		{ 
			"animatedTextureVar" "$basetexture" 
			"animatedTextureFrameNumVar" "$frame" 
			"animatedTextureFrameRate" "12" 
		}
	}
}
